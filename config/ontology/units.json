      "treatment": ["program", "curriculum", "intervention", "tutoring"]
    },
    "retail": {
      "outcome": ["revenue", "sales", "margin", "basket_size", "ltv"],
      "treatment": ["discount", "promotion", "campaign", "pricing"]
    },
    "finance": {
      "outcome": ["return", "profit", "loss", "roi", "sharpe_ratio"],
      "treatment": ["portfolio", "strategy", "allocation", "hedge"]
    },
    "policy": {
      "outcome": ["employment", "income", "poverty_rate", "crime_rate"],
      "treatment": ["policy", "regulation", "subsidy", "tax"]
    },
    "network": {
      "outcome": ["engagement", "virality", "centrality", "diffusion"],
